-- Loader por PlaceId
local games = {
    [3623096087] = "https://pastebin.com/raw/XXXXXXXX", -- Muscle Legends
    [0000000000] = "https://pastebin.com/raw/YYYYYYYY", -- Juego 2
    [5555555555] = "https://pastebin.com/raw/ZZZZZZZZ"  -- Juego 3
}

local placeId = game.PlaceId

if games[placeId] then
    loadstring(game:HttpGet(games[placeId]))()
else
    warn("There is no script for this game (PlaceId: " .. placeId .. ")")
end

